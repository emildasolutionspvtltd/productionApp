    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "open": "^7.0.3",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ]
      },
      "packagerConfig": {
        "asar": true,
        "quite": true
      },
      "makers": [
        {
          "name": "@electron-forge/maker-wix",
          "config": {
            "ui": {
              "name":"Roza Pos",
              "iconUrl":"favicon.ico",
              "chooseDirectory": true
            }
          }
        },
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "noMsi": false,
            "name": "windowsX64_RozaPOS"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
