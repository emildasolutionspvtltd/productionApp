    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/fire": "^6.1.4",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~9.0.0",
    "@angular/material": "^9.2.4",
