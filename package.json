{
  "name": "desktopapp",
  "version": "0.0.0",
  "main": "main.js",
  "author": "kriti kadolkar",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build --base-href ./  && electron main.js",
    "electron-build": "ng build --prod",
    "build-osx": "electron-packager . $npm_package_productName --out=dist/osx --platform=darwin --arch=x64 && npm run codesign",
    "build-win32": "electron-packager . $npm_package_productName --out=dist/win --platform=win32 --arch=ia32 --overwrite",
    "build-win64": "electron-packager . $npm_package_productName --out=dist/win --platform=win32 --arch=x64 --version=0.36.2 app-version=1.0 "
  },
